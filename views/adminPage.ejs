    <div style="padding-top: 3.9%;">
        <div class="container-xl">
            <div class="table-responsive">
                <div class="table-wrapper">
                    <div class="table-title">
                        <div class="row">
                            <div class="col-sm-5">
                                <h2>Students <b>List</b></h2>
                            </div>
                        </div>
                    </div>
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>
                                <th>EmailId</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>

                            <% for(let i=1;i<members.length;i++){ %>

                            <tr>
                                <td><%= i %></td>
                                <td><a href="/adminDashboard/<%= members[i]._id %>">
                                        <%= members[i].name %></a></td>
                                <td><%= members[i].username %></td>
                                <td><span
                                        class=" status text-<%= members[i].verify==null?'warning':(members[i].verify==false?'danger':'success') %>" style="display: inline;">&bull;</span><%= members[i].verify==null?'pending':(members[i].verify==false?'rejected':'accepted') %>
                                </td>
                            </tr>

                            <% } %>

                        </tbody>
                    </table>
                    <div class="clearfix">
                    </div>
                </div>
            </div>

        </div>
    </div>
