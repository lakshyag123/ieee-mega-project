<div id="middle" style="padding-top: 1.9%;">
  <div class="wrapper" id='frm'>

    <form action="/register" method="POST" enctype="multipart/form-data">
      <div class="title">
        Registration Form
      </div>
      <% const rang = message === 'Successfully registered, you will be shortly notified about your verification status through your mail'?'hara':'lal' %>
      <% if(rang == 'lal'){ %>
      <div class="text-center inputfield terms" style="color: red;">
        <% if(message === ''){ %>
        <p></p>
        <% } else {%>
        <p></p><%= message %>
        <% } %>

      </div>
      <% }else{ %>
      <div class="text-center inputfield terms" style="color: green;">
        <p></p><%= message %>
      </div>
      <% } %>
      <div class=" form">

        <div class="inputfield">
          <label>Name</label>
          <input type="text" class="input" name="name" required='required' value="<%= user==null?'':user.name %>">
        </div>
        <div class="inputfield">
          <label>Hostel no.</label>
          <input type="text" class="input" name="hostel_no" required='required'
            value="<%= user==null?'':user.hostel_no %>">
        </div>
        <div class="inputfield">
          <label>Room no.</label>
          <input type="text" class="input" name="room_no" required='required' value="<%= user==null?'':user.room_no %>">
        </div>
        <div class="inputfield">
          <label>Roll no.</label>
          <input type="text" class="input" name="roll_no" required='required' value="<%= user==null?'':user.roll_no %>">
        </div>
        <div class="inputfield">
          <label>Email Address</label>
          <input type="email" class="input" name='username' required='required'
            value="<%= user==null?'':user.username%>">
        </div>
        <div class="inputfield">
          <label>Password</label>
          <input type="password" class="input" name="password" required='required'>
        </div>
        <div class="inputfield">
          <label>Confirm Password</label>
          <input type="password" class="input" name="confirm_password" required='required'>
        </div>

        <div class="inputfield">
          <label>Contact Number</label>
          <input type="tel" class="input" name='phone' required='required' placeholder="with country code ex: +91"
            value="<%= user==null?'':user.phone %>">
        </div>
        <div class="inputfield">
          <label>College ID image</label>
          <input type="file" class="input" name='file' required='required'>
        </div>
        <div class="inputfield">
          <input type="submit" value="Register" class="btn">
        </div>
      </div>
    </form>

    <div class="text-center inputfield terms" style="color: #454646;">
      <p></p>Already have an account? <a href="/" style="color: black;">Sign
        in</a>
    </div>
  </div>

</div>


