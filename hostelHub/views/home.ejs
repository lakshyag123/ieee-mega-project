<div id="middle" style="padding-top: 3.9%;">


  <div class="wrapper" id='frm'>
    <form action="/" method="POST">
      <div class="title">
        Login
      </div>
      <div class="text-center inputfield terms" style="color: red;">
        <p></p><%= message %>
      </div>
      <div class="form">

        <div class="inputfield">
          <label>Email Address</label>
          <input type="email" class="input" name='username' required='required'
            value="<%= user==null?'':user.username %>">
        </div>
        <div class="inputfield">
          <label>Password</label>
          <input type="password" class="input" name="password" required='required'>
        </div>
        <div class="inputfield">
          <input type="submit" value="Login" class="btn">
        </div>
      </div>
    </form>
    <div class="text-center inputfield terms" style="color: #454646;">
      <p></p> <a href="/forgotpassword" style="color: black;">Forgot password?<p></a>
    </div>
  </div>
</div>

